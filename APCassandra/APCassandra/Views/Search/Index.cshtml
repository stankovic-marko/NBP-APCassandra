
@{
    ViewData["Title"] = "Search";
}

<h1>Search</h1>


@foreach (var car in Model)
{
    <hr />
    <div>Brand: <a href="/car/@car.Id">@car.Brand @car.Model</a></div>
    <div>Year: @car.Year</div>
    <div>Type: @car.Type</div>
    <div>Fuel: @car.Fuel</div>
    <div>Brand: @car.Price</div>
    <img src="@car.ShowImage" />
    <hr />
}



